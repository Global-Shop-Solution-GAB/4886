	v.Local.sT120179.Declare
	
	
	V.Local.fOver.Declare
	V.Local.sOver.Declare
	V.Local.fNAA.Declare
	V.Local.sNAA.Declare

	F.Intrinsic.Control.SelectCase(V.Args.Load)
		F.Intrinsic.Control.Case("One")
			F.Data.DataTable.Compute("dtAging","SUM(AMT_INVOICE)","",V.Local.fAmt)
			F.Data.DataTable.Compute("dtAging","SUM(CURRENT1)","",V.Local.f030)
			F.Data.DataTable.Compute("dtAging","SUM(T0130)","",V.Local.f129)
			F.Data.DataTable.Compute("dtAging","SUM(T3059)","",V.Local.fT3059)
			F.Data.DataTable.Compute("dtAging","SUM(T6089)","",V.Local.fT6089)
			F.Data.DataTable.Compute("dtAging","SUM(T90119)","",V.Local.fT90119)
			F.Data.DataTable.Compute("dtAging","SUM(T120179)","",V.Local.fT120179)
			
			F.Data.DataTable.Compute("dtAging","SUM(OVER)","",V.Local.fOver)
			F.Data.DataTable.Compute("dtAging","SUM(NOT_AGED)","",V.Local.fNAA)
			F.Intrinsic.String.Format(V.Local.fAmt,"#,###,###,##0.00",V.Local.sAmt)
			Gui.FrmAging.txtAmt.Text(V.Local.sAmt)
			F.Intrinsic.String.Format(V.Local.f030,"#,###,###,##0.00",V.Local.s030)
			Gui.FrmAging.txt030.Text(V.Local.s030)
			F.Intrinsic.String.Format(V.Local.f129,"#,###,###,##0.00",V.Local.s129)
			Gui.FrmAging.txt0129.Text(V.Local.s129)
			F.Intrinsic.String.Format(V.Local.fT3059,"#,###,###,##0.00",V.Local.sT3059)
			Gui.FrmAging.txt3059.Text(V.Local.sT3059)
			F.Intrinsic.String.Format(V.Local.fT6089,"#,###,###,##0.00",V.Local.sT6089)
			Gui.FrmAging.txt6090.Text(V.Local.sT6089)
			F.Intrinsic.String.Format(V.Local.fT90119,"#,###,###,##0.00",V.Local.sT90119)
			Gui.FrmAging.txt90119.Text(V.Local.sT90119)
			F.Intrinsic.String.Format(V.Local.fT120179,"#,###,###,##0.00",V.Local.sT120179)
			Gui.FrmAging.txt120179.Text(V.Local.sT120179)
			
			
			F.Intrinsic.String.Format(V.Local.fOver,"#,###,###,##0.00",V.Local.sOver)
			gui.FrmAging.txtOver.Text(V.Local.sOver)
			F.Intrinsic.String.Format(V.Local.fNAA,"#,###,###,##0.00",V.Local.sNAA)
			gui.FrmAging.txtNAA.Text(V.Local.sNAA)

		F.Intrinsic.Control.Case("Filter")
			F.Data.DataTable.Compute("dtAging","SUM(AMT_INVOICE)",V.Args.Filter,V.Local.fAmt)
			F.Data.DataTable.Compute("dtAging","SUM(CURRENT1)",V.Args.Filter,V.Local.f030)
			F.Data.DataTable.Compute("dtAging","SUM(T0130)",V.Args.Filter,V.Local.f129)
			F.Data.DataTable.Compute("dtAging","SUM(T3059)",V.Args.Filter,V.Local.fT3059)
			F.Data.DataTable.Compute("dtAging","SUM(T6089)",V.Args.Filter,V.Local.fT6089)
			
			F.Data.DataTable.Compute("dtAging","SUM(T90119)",v.args.filter,V.Local.fT90119)
			F.Data.DataTable.Compute("dtAging","SUM(T120179)",v.args.filter,V.Local.fT120179)
			F.Data.DataTable.Compute("dtAging","SUM(OVER)",V.Args.Filter,V.Local.fOver)
			F.Data.DataTable.Compute("dtAging","SUM(NOT_AGED)",V.Args.Filter,V.Local.fNAA)
			F.Intrinsic.String.Format(V.Local.fAmt,"#,###,###,##0.00",V.Local.sAmt)
			Gui.FrmAging.txtAmt.Text(V.Local.sAmt)
			
